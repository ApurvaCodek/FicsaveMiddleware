<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="Ficsave Page">
        <ListPreference
            android:defaultValue="mobi"
            android:title="Choose Default File Type"
            android:entries="@array/FileTypeToDownload"
            android:entryValues="@array/FileTypeToDownload"
            android:key="default_file_type" />
        <ListPreference
            android:defaultValue="download"
            android:title="Download or Send Email from Ficsave Directly"
            android:entries="@array/DownloadorSendEmail"
            android:entryValues="@array/DownloadorSendEmail"
            android:key="default_site_download_email" />
    </PreferenceCategory>
</PreferenceScreen>